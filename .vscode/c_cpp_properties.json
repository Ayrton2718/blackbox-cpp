{
  "configurations": [
    {
      "name": "Linux",
      "browse": {
        "databaseFilename": "${default}",
        "limitSymbolsToIncludedHeaders": false
      },
      "includePath": [
        "/usr/include/**",
        "${workspaceFolder}/**",
        "${workspaceFolder}/install/**",
        "${workspaceFolder}/install/proto/**",
        "~/.conan2/**"
        
      ],
      "intelliSenseMode": "gcc-x64",
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "gnu11",
      "cppStandard": "c++17"
    }
  ],
  "version": 4
}