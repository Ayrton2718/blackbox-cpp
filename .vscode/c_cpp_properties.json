{
    "configurations": [
        {
            "name": "Linux",
            "browse": {
                "databaseFilename": "${default}",
                "limitSymbolsToIncludedHeaders": false
            },
            "includePath": [
                "/usr/include/**",
                "${workspaceFolder}/inc/**",
                "${workspaceFolder}/install/**",
                "~/.conan2/**"
            ],
            "intelliSenseMode": "gcc-x64",
            "compilerPath": "/usr/bin/g++",
            "cStandard": "gnu11",
            "cppStandard": "c++17"
        }
    ],
    "version": 4
}